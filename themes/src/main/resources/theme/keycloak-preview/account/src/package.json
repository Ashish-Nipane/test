{
  "name": "keycloak-preview",
  "version": "1.0.0",
  "description": "keycloak-preview account management written in React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "snowpack --optimize && npm run check-types && npm run babel && npm run move-web_modules",
    "post-module-build": "purgecss --css dist/index.css --content dist/**/*.js --output dist/index.css",
    "build-module": "rollup -c && npm run post-module-build",
    "babel": "babel --source-maps --extensions \".js,.ts,.tsx\" app/ --out-dir ../resources/",
    "babel:watch": "npm run babel -- --watch",
    "check-types": "tsc --noImplicitAny --strictNullChecks --jsx react -p ./",
    "check-types:watch": "npm run check-types -- -w",
    "lint": "eslint ./app/**/*.ts*",
    "move-web_modules": "shx mv web_modules ../../../keycloak/common/resources"
  },
  "keywords": [],
  "author": "Stan Silvert",
  "license": "Apache-2.0",
  "dependencies": {
    "@patternfly/react-core": "^3.153.3",
    "@patternfly/react-icons": "^3.15.16",
    "@patternfly/react-styles": "^3.7.14",
    "react": "npm:@pika/react@^16.13.1",
    "react-dom": "npm:@pika/react-dom@^16.13.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "shx": "^0.3.2",
    "snowpack": "^1.7.0",
    "tslib": "^2.0.0",
    "typescript": "^3.8.3"
  },
  "repository": {}
}
